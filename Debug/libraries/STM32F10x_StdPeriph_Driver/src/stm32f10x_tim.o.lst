 541:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Get the TIMx CCER register value */
 542:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccer = TIMx->CCER;
 543:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Get the TIMx CR2 register value */
 544:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpcr2 =  TIMx->CR2;
 545:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 546:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Get the TIMx CCMR2 register value */
 547:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccmrx = TIMx->CCMR2;
 548:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     
 549:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Reset the Output Compare mode and Capture/Compare selection Bits */
 550:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccmrx &= (uint16_t)(~((uint16_t)TIM_CCMR2_OC4M));
 551:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccmrx &= (uint16_t)(~((uint16_t)TIM_CCMR2_CC4S));
 552:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 553:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Select the Output Compare Mode */
 554:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccmrx |= (uint16_t)(TIM_OCInitStruct->TIM_OCMode << 8);
 555:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 556:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Reset the Output Polarity level */
 557:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccer &= (uint16_t)(~((uint16_t)TIM_CCER_CC4P));
 558:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Set the Output Compare Polarity */
 559:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccer |= (uint16_t)(TIM_OCInitStruct->TIM_OCPolarity << 12);
 560:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 561:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Set the Output State */
 562:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   tmpccer |= (uint16_t)(TIM_OCInitStruct->TIM_OutputState << 12);
 563:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     
 564:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   if((TIMx == TIM1) || (TIMx == TIM8))
 565:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   {
 566:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     assert_param(IS_TIM_OCIDLE_STATE(TIM_OCInitStruct->TIM_OCIdleState));
 567:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     /* Reset the Output Compare IDLE State */
 568:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     tmpcr2 &= (uint16_t)(~((uint16_t)TIM_CR2_OIS4));
 569:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     /* Set the Output Idle state */
 570:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****     tmpcr2 |= (uint16_t)(TIM_OCInitStruct->TIM_OCIdleState << 6);
 571:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   }
 572:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Write to TIMx CR2 */
 573:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   TIMx->CR2 = tmpcr2;
 574:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 575:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Write to TIMx CCMR2 */  
 576:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   TIMx->CCMR2 = tmpccmrx;
 577:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c **** 
 578:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Set the Capture Compare Register value */
 579:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   TIMx->CCR4 = TIM_OCInitStruct->TIM_Pulse;
 580:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   
 581:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   /* Write to TIMx CCER */
 582:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c ****   TIMx->CCER = tmpccer;
 583:../libraries/STM32F10x_StdPeriph_Driver/src/stm32f10x_tim.c **** }
